<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper SYSTEM "mybatis-3-mapper.dtd">

<mapper namespace="co.yedam.payment.mapper.PaymentMapper">

	<insert id="insert" parameterType="co.yedam.payment.service.PaymentVO">
		insert into payment
		values(#{pid},seq_payment.nextval,#{adcnt}, #{chcnt}, #{fcode})
	</insert>

<select id="selectList" resultType="co.yedam.payment.service.PaymentVO" parameterType="string">
	select * from payment
	
</select>
</mapper>